{
  "name": "data-i18n",
  "version": "1.2.1",
  "description": "Most powerful Javascript i18n Translations manager",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/data-i18n/data-i18n.js",
  "repository": {
    "url": "https://github.com/data-i18n/data-i18n.git"
  },
  "keywords": [
    "i18n",
    "translations",
    "i18n javascript",
    "i18n html",
    "i18n react",
    "i18n angular",
    "i18n vue.js",
    "i18n stencil.js",
    "web component",
    "localization",
    "translate"
  ],
  "author": {
    "name": "Miquel Martin",
    "email": "info@data-i18n.com"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "echo 'Running linter, please wait...' && (npm run lint-plain || (npm run lint-report; exit -1))",
    "lint-plain": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx src/ --ignore-path .eslintignore",
    "lint-report": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx src/ --ignore-path .eslintignore -f ./node_modules/eslint-html-reporter/reporter.js -o logs/eslint-report.html || opn ./logs/eslint-report.html"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.5",
    "jest-cli": "^27.2.5",
    "pre-push": "^0.1.1",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {},
  "pre-push": [
    "lint"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
